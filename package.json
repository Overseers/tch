{
  "name": "@overseers/tch",
  "version": "0.0.33",
  "description": "Tedious Connection Handler",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && npm run watch",
